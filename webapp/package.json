{
  "name": "webapp",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@types/history": "^4.6.0",
    "@types/jest": "^20.0.4",
    "@types/node": "^8.0.14",
    "@types/react": "^15.0.38",
    "@types/react-bootstrap": "^0.0.52",
    "@types/react-dom": "^15.5.1",
    "@types/react-router": "^4.0.14",
    "@types/react-router-redux": "^5.0.8",
    "@types/redux": "^3.6.0",
    "@types/redux-actions": "^1.2.8",
    "bootstrap": "3",
    "node-sass-chokidar": "^0.0.3",
    "npm-run-all": "^4.1.1",
    "react": "^15.6.1",
    "react-bootstrap": "^0.31.1",
    "react-dom": "^15.6.1",
    "react-materialize": "^1.0.14",
    "react-redux": "^5.0.5",
    "react-router-dom": "^4.1.2",
    "react-router-redux": "next",
    "react-scripts-ts": "2.4.0",
    "redux": "^3.7.2",
    "redux-actions": "^2.2.1",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "enzyme": "^3.0.0",
    "enzyme-adapter-react-15": "^1.0.0",
    "jest-enzyme": "^4.0.0",
    "ncp": "^2.0.0",
    "react-test-renderer": "^16.0.0",
    "rimraf": "^2.6.2"
  },
  "scripts": {
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "start-js": "react-scripts-ts start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-scripts-ts build",
    "postbuild": "npm run copy-to-wwwroot",
    "test": "react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject",
    "clean-server": "rimraf ../server/BagAPI/wwwroot",
    "copy-to-wwwroot": "npm run clean-server && ncp ./build ../server/BagAPI/wwwroot"
  },
  "proxy": "http://localhost:5000"
}
